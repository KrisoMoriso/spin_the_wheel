{
  "manifest_version": 2,
  "name": "Spin the wheel",
  "version": "1.0",


  "content_scripts": [
    {
      "matches": ["*://*.wheelofnames.com/*"],
      "js": ["spin_the_wheel.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "*://*.wheelofnames.com/*"
  ]
}